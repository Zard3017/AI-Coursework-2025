    "def days_seconds():\n",
    "    days = int(input(\"Enter number of days: \"))\n",
    "    seconds = days * 24 * 60 * 60\n",
    "    print(f\"{days} days = {seconds} seconds\")\n",
    "\n",
    "# Call the function\n",
    "days_seconds()\n"
